<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">

  <modelVersion>4.0.0</modelVersion>

  <!-- Collegamento al parent -->
  <parent>
    <groupId>kernel-nttdata-ncp-parent</groupId>
    <artifactId>kernel-nttdata-catalogoprofili-parent</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <relativePath>../kernel-nttdata-catalogoprofili-parent/pom.xml</relativePath>
  </parent>

  <!-- Identificativo del modulo -->
  <groupId>kernel-nttdata-ncp-parent</groupId>
  <artifactId>kernel-nttdata-catalogoprofili-services</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <packaging>jar</packaging>

  <name>Kernel NTTDATA Catalogo Profili - Services</name>
  <description>Modulo di logica di business per la gestione dei profili</description>

  <!-- Forza Java 21 -->
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.11.0</version>
        <configuration>
          <source>21</source>
          <target>21</target>
          <release>21</release>
        </configuration>
      </plugin>
    </plugins>
  </build>

</project>
